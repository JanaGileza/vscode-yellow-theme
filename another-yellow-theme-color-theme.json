		"button.hoverBackground": "#e1dfb3",
		
		"checkbox.background": "#fdfbd8",
		"checkbox.foreground": "#ffc400",
		"checkbox.border": "#926c05",

		"dropdown.background": "#ffc400",
		"dropdown.listBackground": "#926c05",
		"dropdown.border": "#926c05",
		"dropdown.foreground": "#fdfbd8",
		
		"editor.background": "#FFFDD0",
		"editor.foreground": "#926c05",
		
