		"activityBarBadge.background": "#ffc400",
		"activityBar.activeBorder": "#dba400",
		"activityBar.activeBackground": "#e8ae00",
		"activityBar.background": "#ffc400",
		"activityBar.border": "#dba400",
		"activityBar.foreground": "#fdfbd8",
		
		"button.background": "#fdfbd8",
